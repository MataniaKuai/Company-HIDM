            <span class="input-group-addon">主题:</span>
            <input id="title" class="form-control">
            </div>
          </div>
          <div class="col-sm-3">
            <div class="input-group">
            <span class="input-group-addon">时间:</span>
            <input id="courseDate" class="form-control">
            </div>
            </div>
          <div class="col-sm-2">
            <button id="search-file-btn" class="btn btn-primary" type="button" onclick="Search()">搜索</button>
          </div>
        </div>
	</div>
	
	<div class="btn-group" style="margin-top:2px;margin-bottom:2px">
        <button type="button" class="btn btn-primary" onclick="view()">查看</button>
    </div>
	<table id="mycourseTable"></table>
	</div>
	
