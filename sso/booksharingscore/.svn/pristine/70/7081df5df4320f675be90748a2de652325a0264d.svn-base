	<script type="text/javascript" src="js/business/mycourse.js"></script>
	<script type="text/javascript">
	    
		
		$(function(){
			$('#mycourseTable').bootstrapTable({
				pagination: true,
				pageNumber:1,
				pageSize:10,
				pageList: [1, 10, 25, 50],
				cache:false,
				sidePagination: "server",
				height:530,
			    columns: [{
			        checkbox: true,
			        select: false,
			    }, {
			        field: 'title',
			        title: '主题'
			    }, {
			        field: 'courseDate',
			        title: '时间'
			    }, {
			        field: 'address',
			        title: '地点'
			    }, {
			        field: 'score',
			        title: '最终得分',
			    }, {
			        field: 'operatert',
			        title: 'id',
			        visible: false
			    }],
			    url : '/booksharingscore/mycourse/search',
			    queryParams : function (params) {
                    var temp = {   
                        rows: params.limit,                         //页面大小
                        page: (params.offset / params.limit) + 1,   //页码
                        sort: params.sort,      //排序列名  
                        sortOrder: params.order //排位命令（desc，asc） 
                    };
                    return temp;
                },
		        clickToSelect: true
			});
			
			$('.form_date').datetimepicker({
				format: 'yyyy-mm-dd',
		        language:  'zh-CN',
		        weekStart: 1,
		        todayBtn:  1,
				autoclose: 1,
				todayHighlight: 1,
				startView: 2,
				minView: 2,
				forceParse: 0
		    });
		});
	</script>
	
	<h4>我的课程列表</h4>
	
	<div id="listDiv">
	<div id="searchDiv">
