<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<title>德淮半导体</title>
	<link rel="stylesheet" type="text/css" href="vendors/bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" href="vendors/bootstrap-table/1.12.1/bootstrap-table.min.css">
	<link rel="stylesheet" href="vendors/bootstrap-select-1.12.4/dist/css/bootstrap-select.min.css">
	<script type="text/javascript" src="vendors/jquery/jquery.min.js"></script>
	<script type="text/javascript" src="vendors/bootstrap/dist/js/bootstrap.min.js"></script>
	
	<style type="text/css">
	    input {
	        margin-bottom: 3px;
	    }
	</style>
	<script type="text/javascript">
        function logout() {
            self.location='/filterService/logout'; 
        }
        
        function login() {
            self.location='/filterService/login'; 
        }
        
        function openSys1() {
            $.ajax({
                url: '/filterService/getUser',
                type: 'POST',
                cache: false,
                async:false,
                success: function (data) {
                    self.location.href = 'Webshell://' + data;
                }
            });
        }
    </script>
</head>

<body style="margin:5px;">
  <div style="margin-bottom:0; height:70px;">
    <div class="row">
          <div class="col-md-4">
              <img src="images/logo.png"  alt="HiDM" />
          </div>
          <div class="col-md-4 text-center">
              <font size="8">SSO系统</font>
          </div>
          <div class="col-md-4">
               <#if islogin >
                  <a href="#" onclick="logout()" class="pull-right" style="margin-top:50px;margin-right:10px">注销</a>
               <#else>
                  <a href="#" onclick="login()" class="pull-right" style="margin-top:50px;margin-right:10px">登录</a>
               </#if>
               <label class="pull-right" style="margin-top:50px;margin-right:30px">${userName}</label>
          </div>
    </div>
  </div>
  <div class="container col-md-4 col-md-offset-4" style="margin-top:100px">
    <a href="#" onclick="openSys1()" style="font-size:30px;font-weigh:bolder">SSO系统1</a><br/>
    <a href="#" onclick="" style="font-size:30px;font-weigh:bolder">SSO系统2</a><br/>
    <a href="#" onclick="" style="font-size:30px;font-weigh:bolder">SSO系统3</a><br/>
    <a href="#" onclick="" style="font-size:30px;font-weigh:bolder">SSO系统4</a><br/> 
  </div>
</body>
</html>