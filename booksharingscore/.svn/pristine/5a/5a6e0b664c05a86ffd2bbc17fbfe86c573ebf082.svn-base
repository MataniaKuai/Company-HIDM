    <div class="panel panel-default" id="dataDiv" style="display:none;">
		<div class="panel-heading">
			<h3 class="panel-title">
				课程信息
			</h3>
		</div>
		<div class="panel-body">
		    <ul id="myTab" class="nav nav-tabs">
		      <li class="active">
		        <a href="#home" data-toggle="tab">
			                  基本信息
		        </a>
	          </li>
	        </ul>
	        <div id="myTabContent" class="tab-content">
	          <div class="tab-pane fade in active" id="home">
			    <form id="editForm" class="form-horizontal" role="form">

                <div class="form-group">
                    <label class="col-sm-2 control-label">主题</label>
                    <div class="col-sm-3">
                       <input type="text" name="title" class="form-control">
                    </div>
                    <label class="col-sm-2 control-label">时间</label>
                    <div class="col-sm-3">
                        <input class="form-control form_date" name="courseDate" type="text" value="">
                    </div>
             
                </div>
                
                <div class="form-group">
                    <label class="col-sm-2 control-label">讲师姓名</label>
                    <div class="col-sm-3"><input type="text" name="lecturerName" class="form-control"></div>
                    
                    <label class="col-sm-2 control-label">讲师工号</label>
                    <div class="col-sm-3"><input type="text" name="lecturerNo" class="form-control"></div>
                    
                </div>
                
                <div class="form-group">
                    <label class="col-sm-2 control-label">地点</label>
                    <div class="col-sm-3">
                        <input type="text" name="address" class="form-control">
                    </div>
                    
                    <label class="col-sm-2 control-label">演讲资料</label>
				    <div class="row">
				    	<div class="col-sm-3">
				            <div class="input-group">
				            <span class="input-group-addon">选择资料:</span>
				            <input id="fileName" name="fileName" type="file" class="form-control">
				            </div>
          				</div>
          				<div class="col-sm-1">
            				<button id="upload-file-btn" class="btn btn-primary" type="button" onclick="upload()">上传</button>
         				</div>
				    </div>
                </div>
                
 	            <input type="hidden" name="id">
                
                <div class="panel-footer text-right">
			        <button type="button" class="btn btn-default" onclick="closeDataDiv()">返回</button>
			        <button id="saveBtn" type="button" class="btn btn-primary" onclick="save()">保存</button>
		        </div>
                </form>
              </div>
              <div class="tab-pane fade" id="items">
                
                <div class="panel-footer text-right">
			        <button type="button" class="btn btn-default" onclick="closeDataDiv()">返回</button>
		        </div>
              </div>
            </div>
		</div>
	</div>