    <div class="modal fade" id="dataModal" data-backdrop="false" tabindex="-1" role="dialog" aria-labelledby="modalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="modalLabel">
                                              Model
                </h4>
            </div>
            <div class="modal-body">
                <form id="editForm" class="form-horizontal" role="form">
                <div class="form-group">
                	<div id="modelDiv">
                    <label class="col-sm-2 control-label">No</label>
                    <div class="col-sm-4">
                        <input type="text" name="modelNo" class="form-control"
                            required data-bv-notempty-message=""
                            data-bv-stringlength="true"
                        >
                    </div>
                    </div>
                    <label class="col-sm-1 control-label">Model</label>
                    <div class="col-sm-4">
                        <input type="text" name="model" class="form-control"
                            required data-bv-notempty-message="Model不能为空"
                            data-bv-stringlength="true"
                        >
                    </div>
                </div>
                    <input type="hidden" name="id">
                    <input type="hidden" name="loginName" value="${(user.userName)!''}"/>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                <button id="saveBtn" type="button" class="btn btn-primary" onclick="save()">保存</button>
            </div>
        </div>
    </div>
    </div>