
<div class="panel panel-default" id="dataDiv" style="display: none;">
	<div class="panel-heading">
		<h3 class="panel-title">IE信息</h3>
	</div>
	<div class="panel-body">
		<ul id="myTab" class="nav nav-tabs">
			<li class="active"><a href="#home" data-toggle="tab"> 基本信息 </a>
			</li>
		</ul>
		<div id="myTabContent" class="tab-content">
			<div class="tab-pane fade in active" id="home">
				<form id="editForm" class="form-horizontal" role="form">
					<div class="form-group">

						<label class="col-sm-1 control-label">EQ Type</label>
						<div class="col-sm-2">
							<input type="text" name="eqTypeId" class="form-control">
						</div>

						<label class="col-sm-1 control-label">Module</label>
						<div class="col-sm-2">
							<input type="text" name="module" class="form-control">
						</div>

						<label class="col-sm-1 control-label">Vendor</label>
						<div class="col-sm-2">
							<input type="text" name="vendor" class="form-control">
						</div>
						<label class="col-sm-1 control-label">Model</label>
						<div class="col-sm-2">
							<input type="text" name="model" class="form-control">
						</div>
					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Length(m)</label>
						<div class="col-sm-3">
							<input type="text" name="length" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Width(m)</label>
						<div class="col-sm-3">
							<input type="text" name="width" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Housing_Main tool(m²)</label>
						<div class="col-sm-3">
							<input type="text" name="housing" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Man-Machine(Maintenance)</label>
						<div class="col-sm-3">
							<input type="text" name="manMachine" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Shift(Maintenance)</label>
						<div class="col-sm-3">
							<input type="text" name="shift" class="form-control">
						</div>

						<label class="col-sm-3 control-label">UPH(unit per hour)</label>
						<div class="col-sm-3">
							<input type="text" name="uph" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Uptime Rate(%)</label>
						<div class="col-sm-3">
							<input type="text" name="uptimeRate" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Rework Rate(%)</label>
						<div class="col-sm-3">
							<input type="text" name="reworkRate" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Wafer Cost(Rework)</label>
						<div class="col-sm-3">
							<input type="text" name="waferCostRework" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Wafer Cost(Scrap)</label>
						<div class="col-sm-3">
							<input type="text" name="waferCostScrap" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Installation and Hookup Cost</label>
						<div class="col-sm-3">
							<input type="text" name="installationHookupCost" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Installation Cost</label>
						<div class="col-sm-3">
							<input type="text" name="installationCost" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Hookup Cost</label>
						<div class="col-sm-3">
							<input type="text" name="hookupCost" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Qualification Cost</label>
						<div class="col-sm-3">
							<input type="text" name="qualificationCost" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Parts Cost</label>
						<div class="col-sm-3">
							<input type="text" name="partsCost" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Man-Machine(Operational)</label>
						<div class="col-sm-3">
							<input type="text" name="operational" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Operater Shift</label>
						<div class="col-sm-3">
							<input type="text" name="operatert" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Operate Labor Salary</label>
						<div class="col-sm-3">
							<input type="text" name="operateLaborSalary" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Maintain Labor Salary</label>
						<div class="col-sm-3">
							<input type="text" name="maintainLaborSalary" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Operate Labor Cost</label>
						<div class="col-sm-3">
							<input type="text" name="operateLaborCost" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Maintain Labor Cost</label>
						<div class="col-sm-3">
							<input type="text" name="maintainLaborCost" class="form-control">
						</div>

					</div>
					
					<div class="form-group">
						<label class="col-sm-2 control-label">Floor Cost (¥/m2)</label>
						<div class="col-sm-3">
							<input type="text" name="floorCost" class="form-control">
						</div>

						<label class="col-sm-3 control-label">Floor Cost(¥)</label>
						<div class="col-sm-3">
							<input type="text" name="floorCostAcount" class="form-control">
						</div>

					</div>

					<div class="input-group" id="materialsGroup">
						<label class="input-group-addon">Utility配置:</label>
						<div id = "displayUtility">
					    </div>

					</div>
					<br />
					<div class="input-group" id="materialsGroup">
						<label class="input-group-addon">Materials配置:</label>
						<div id = "displayMaterial">
					    </div>

					</div>

					<input type="hidden" name="id">
					<input type="hidden" name="userBaseId">
					<div class="panel-footer text-right">
						<button type="button" class="btn btn-default"
							onclick="closeDataDiv()">返回</button>
<!-- 						<button id="saveBtn" type="button" class="btn btn-primary" -->
<!-- 							onclick="save()">计算</button> -->
					</div>
				</form>
			</div>
		</div>
	</div>